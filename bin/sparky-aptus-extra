if [ "$ARCHARM" != "" ]; then
	$DIALOG $TEXT"\n$LOCAL89\n\n$LOCAL90" $TITLE"$TITLETEXT" $MSGBOX
	exit 1
fi
if [ -f /usr/bin/grub-customizer ]; then
	$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTSTOP"
	mainmenu
else
	$SPARKYXTERM "$INSTALL grub-customizer"
	if [ -f /usr/bin/grub-customizer ]; then
		$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTDONE"
		mainmenu
	else
		$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTBREAK"
	fi
