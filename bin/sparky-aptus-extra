}

gearthmenu () {
if [ "$ARCHARM" != "" ]; then
	$DIALOG $TEXT"\n$LOCAL89\n\n$LOCAL90" $TITLE"$TITLETEXT" $MSGBOX
	exit 1
fi
if [ -f /usr/bin/google-earth-pro ] || [ -f /usr/bin/googleearth ]; then
	$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTSTOP"
	mainmenu
else
	/usr/lib/sparky-aptus-extra/bin/install-earth
fi
mainmenu
}

