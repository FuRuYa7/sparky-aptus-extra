Sparky_Screenshot "$LOCAL40" "$SHOT1" \
Sparky_Tray "$LOCAL41" "$TRAY1" \
Sparky_USB_Formatter "$LOCAL42" "$FORMATTER1" \
Sparky_Users "$LOCAL43" "$USER1" \
Sparky_WWW "$LOCAL88" "$WEBBROW1" \
Sparky_Xdf "$LOCAL44" "$XDF1" \
SpiderOak "$LOCAL46" "$SPIDER1" \
Spotify "$LOCAL84" "$SPOTIFY1" \
Steam "$LOCAL47" "$STEAM1" \
TeamSpeak "$LOCAL95" "$TSPEAK1" \
TeamViewer "$LOCAL48" "$TEAM1" \
TOR_Messenger "$LOCAL69" "$TORMES1" \
Trojita "$LOCAL83" "$TROJITA1" \
TrueCrypt "$LOCAL50" "$TRUE1" \
Variety "$LOCAL51" "$VARIETY1" \
VeraCrypt "$LOCAL52" "$VERA1" \
VMware_Player "$LOCAL53" "$VPLAYER1" \
Vuze "$LOCAL54" "$VUZE1" \
WoeUSB "$LOCAL97" "$WOE1" \
XnViewMP "$LOCAL55" "$XNVIEW1" \
YouTube-DLG "$LOCAL77" "$YOU1"`

#DDM "$LOCAL13" "$DDM1" \
#LightDM_Manager "$LOCAL20" "$LIGHTDM1" \

if [ "$?" = "0" ]; then
	CHOICE=`echo $CHOICES | cut -d "|" -f 1`
else
	exit 0
fi

if [ "$CHOICE" = "BleachBit" ]; then
	bleachmenu
elif [ "$CHOICE" = "BlueGriffon" ]; then
	bluegrmenu
elif [ "$CHOICE" = "Ceni" ]; then
	cenimenu
elif [ "$CHOICE" = "Codecs" ]; then
	codecsmenu
elif [ "$CHOICE" = "Custom_ISO_Builder" ]; then
	cibmenu
#elif [ "$CHOICE" = "DDM" ]; then
#	ddmmenu
elif [ "$CHOICE" = "DEB_Package_Builder" ]; then
	debmenu
elif [ "$CHOICE" = "Dropbox" ]; then
	dropmenu
elif [ "$CHOICE" = "Eraser" ]; then
	erasermenu
elif [ "$CHOICE" = "Google-Earth" ]; then
	gearthmenu
elif [ "$CHOICE" = "Gradio" ]; then
	gradiomenu
elif [ "$CHOICE" = "Grub_Customizer" ]; then
	grubmenu
elif [ "$CHOICE" = "GTomb" ]; then
	gtombmenu
elif [ "$CHOICE" = "Leo" ]; then
	leomenu
#elif [ "$CHOICE" = "LightDM_Manager" ]; then
#	lightdmmenu
elif [ "$CHOICE" = "LightScribe" ]; then
	lightscribemenu
elif [ "$CHOICE" = "LightZone" ]; then
	lightzonemenu
elif [ "$CHOICE" = "Live_USB_Creator" ]; then
	livemenu
elif [ "$CHOICE" = "Lxmed" ]; then
	lxmedmenu
elif [ "$CHOICE" = "Master_PDF_Editor" ]; then
	masterpdfmenu
elif [ "$CHOICE" = "MEGAsync" ]; then
	megamenu
elif [ "$CHOICE" = "Mintstick" ]; then
	mstickmenu
elif [ "$CHOICE" = "MotionBox" ]; then
	motionmenu
elif [ "$CHOICE" = "Notepadqq" ]; then
	notepadmenu
elif [ "$CHOICE" = "Office" ]; then
	officemenu
elif [ "$CHOICE" = "OpenToonz" ]; then
	opentoonzmenu
elif [ "$CHOICE" = "PMRP" ]; then
	pmrpmenu
elif [ "$CHOICE" = "QMPlay2" ]; then
	qmplaymenu
elif [ "$CHOICE" = "RSS_Guard" ]; then
	rssgmenu
elif [ "$CHOICE" = "RSSOwl" ]; then
	owlmenu
elif [ "$CHOICE" = "Sayonara" ]; then
	sayonaramenu
elif [ "$CHOICE" = "Skype" ]; then
	skypemenu
elif [ "$CHOICE" = "Sparky_APTus" ]; then
	aptusmenu
elif [ "$CHOICE" = "Sparky_APTus_Gamer" ]; then
	aptgamermenu
