bleachmenu () {
if [ -f /usr/bin/bleachbit ]; then
	$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTSTOP"
	mainmenu
else
	$SPARKYXTERM "$INSTALL bleachbit"
	if [ -f /usr/bin/bleachbit ]; then
		$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTDONE"
		mainmenu
	else
		$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTBREAK"
	fi
fi
mainmenu
}

bluegrmenu() {
if [ "$ARCHARM" != "" ]; then
	$DIALOG $TEXT"\n$LOCAL89\n\n$LOCAL90" $TITLE"$TITLETEXT" $MSGBOX
	exit 1
fi
if [ -f /usr/bin/bluegriffon ]; then
	$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTSTOP"
	mainmenu
else
	$SPARKYXTERM "$INSTALL bluegriffon"
	if [ -f /usr/bin/bluegriffon ]; then
		$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTDONE"
		mainmenu
	else
		$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTBREAK"
	fi
fi
mainmenu
}

cenimenu () {
if [ -f /usr/bin/Ceni ]; then
	$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTSTOP"
	mainmenu
else
	$SPARKYXTERM "$INSTALL ceni"
	if [ -f /usr/bin/Ceni ]; then
		$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTDONE"
		mainmenu
	else
		$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTBREAK"
	fi
fi
mainmenu
}

codecsmenu () {
if [ -f /usr/bin/sparky-codecs ]; then
	/usr/bin/sparky-codecs
else
	$SPARKYXTERM "$INSTALL sparky-codecs"
	if [ -f /usr/bin/sparky-codecs ]; then
		/usr/bin/sparky-codecs
	else
		$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTBREAK"
	fi
fi
mainmenu
}

cibmenu () {
if [ -f /usr/bin/custom-iso-builder ]; then
	$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTSTOP"
	mainmenu
else
	$SPARKYXTERM "$INSTALL custom-iso-builder"
	if [ -f /usr/bin/custom-iso-builder ]; then
		$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTDONE"
		mainmenu
	else
	$DIALOG $TITLE"$TITLETEXT" $MSGBOX $TEXT"$TEXTBREAK"
	fi
fi
mainmenu
}

